clc; clear all;
coeff = [2.76, -3.81, 2.65, -0.92];

model = arima('constant', 0, 'AR', coeff, 'variance', 1);
x = simulate(model,1000);
x = x(500:end);
[h_ideal, w_ideal] = freqz(1, [1 -coeff],500)
p_2 = aryule(x,2)

title('Log of PSD Estimate of Noisy Sinewave')
plot(w1/max(w1), Pxx);
xlabel('Normalised Frequency')
ylabel('Power')
grid on


